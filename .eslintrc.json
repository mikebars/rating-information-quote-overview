      "rules": {
        // ESLint rule enforcing comment capitalization.
        "capitalized-comments": [
          "error",
          "always",
          {
            "ignoreConsecutiveComments": false,
            "ignoreInlineComments": false,
            "ignorePattern": "tslint"
          }
        ],

        // ESLint rule enforcing allowed length of identifiers.
        "id-length": "off",

        // ESLint rule enforcing maximum allowed lines per file.
        "max-lines": "off",

        // ESLint rule enforcing maximum allowed lines per function.
        "max-lines-per-function": "off",

        // ESLint rule enforcing maximum allowed statements per function.
        "max-statements": "off",

        // ESLint rule enforcing disuse of 'magic' numbers.
        "no-magic-numbers": "off",

        // ESLint rule enforcing disuse of ternary operators.
        "no-ternary": "off",

        // ESLint rule enforcing disuse of 'undefined'.
        "no-undefined": "off",

        // ESLint rule enforcing import sorting.
        "sort-imports": "off",

        // ESLint rule enforcing consistent definition of array types using either T[] or Array<T>.
        "@typescript-eslint/array-type": [
          "error",
          {
            // Sets the array type expected for mutable cases.
            "default": "generic",

            // Sets the array type expected for readonly arrays.
            // If this is omitted, then the value for default will be used.
            "readonly": "generic"
          }
        ],

        // ESLint rule enforcing consistent definition of types using either interface or type.
        "@typescript-eslint/consistent-type-definitions": ["error", "type"],

        // ESLint rule enforcing disusage of 'require' statements except in import statements.
        "@typescript-eslint/no-var-requires": "off",

        // Runs TSLint as an ESLint rule and reports issues as individual ESLint issues.
        // "@typescript-eslint/tslint/config": [
        //   "warn",
        //   {
        //     // Path to tslint.json of your project.
        //     "lintFile": "./tslint.json"

        //     // tslint rules (will be used if `lintFile` is not specified).
        //     // "rules": {},

        //     // Array of paths to directories with rules, e.g. 'node_modules/tslint/lib/rules' (will be used if `lintFile` is not specified).
        //     // "rulesDirectory": []
        //   }
        // ],

        // ESLint rule enforcing imported modules can be resolved to a module on the local filesystem,
        // as defined by standard Node 'require.resolve' behavior.
        "import/no-unresolved": "off",

        // ESLint rule enforcing snapshots are not too large.
        "jest/no-large-snapshots": "off",

        // ESLint rule enforcing imported modules exist.
        "node/no-missing-import": [
          "error",
          {
            // Additional modules to allow.
            // "allowModules": [],

            // Additional paths to try when resolving imports.
            "resolvePaths": ["."],

            // File extensions to try when resolving imports.
            "tryExtensions": [
              ".cjs",
              ".d.ts",
              ".js",
              ".json",
              ".json5",
              ".jsx",
              ".mjs",
              ".node",
              ".ts",
              ".tsx"
            ]
          }
        ],

        // ESLint rule enforcing disusage of unsupported ECMAScript syntax.
        "node/no-unsupported-features/es-syntax": [
          "error",
          {
            // The ECMAScript syntax features to ignore.
            "ignores": [
              // ECMAScript 2015
              "arrowFunctions",
              "binaryNumericLiterals",
              "blockScopedFunctions",
              "blockScopedVariables",
              "classes",
              "computedProperties",
              "defaultParameters",
              "destructuring",
              "forOfLoops",
              "generators",
              "modules",
              "new.target",
              "objectSuperProperties",
              "octalNumericLiterals",
              "propertyShorthands",
              "regexpU",
              "regexpY",
              "restParameters",
              "spreadElements",
              "templateLiterals",
              "unicodeCodePointEscapes",

              // ECMAScript 2016
              "exponentialOperators",

              // ECMAScript 2017
              "asyncFunctions",
              "trailingCommasInFunctions",

              // ECMAScript 2018
              "asyncIteration",
              "malformedTemplateLiterals",
              "regexpLookbehind",
              "regexpNamedCaptureGroups",
              "regexpS",
              "regexpUnicodeProperties",
              "restSpreadProperties",

              // ECMAScript 2019
              "jsonSuperset",
              "optionalCatchBinding",

              // ECMAScript 2020
              "bigint",
              "dynamicImport"
            ],

            // The version range of Node that is supported.
            "version": ">=8.0.0"
          }
        ],

        // Runs Prettier as an ESLint rule and reports differences as individual ESLint issues.
        "prettier/prettier": "error",

        // ESLint rule enforcing consistent usage of destructuring assignment of props, state, and context.
        "react/destructuring-assignment": [
          "error",
          "never",
          {
            // Ignore in class field declarations.
            "ignoreClassFields": false
          }
        ],

        // ESLint rule enforcing a specific function type for function components.
        "react/function-component-definition": [
          "error",
          {
            // Function type for named function components.
            "namedComponents": "arrow-function",

            // Function type for unnamed function components.
            "unnamedComponents": "arrow-function"
          }
        ],

        // ESLint rule enforcing allowed extensions that may contain JSX.
        "react/jsx-filename-extension": [
          "error",
          {
            "allow": "always",
            "extensions": [".js", ".jsx", ".tsx"]
          }
        ],

        // ESLint rule enforcing the allowed max depth in JSX.
        "react/jsx-max-depth": "off",

        // ESLint rule enforcing the disusage of string literals in JSX.
        "react/jsx-no-literals": "off",

        // Prevent React to be incorrectly marked as unused.
        "react/jsx-uses-react": "off",

        // Prevent missing React when using JSX.
        "react/react-in-jsx-scope": "off",

        // ESLint rule enforcing immediately returning a value rather than declaring a variable and returning it.
        "sonarjs/prefer-immediate-return": "off",

        // ESLint rule enforcing the casing for filenames.
        "unicorn/filename-case": [
          "error",
          {
            // The case to use ('camelCase', 'kebabCase', 'pascalCase', 'snakeCase').
            // "case": "kebabCase",

            // The cases to allow or disallow.
            "cases": {
              // Allow or disallow camelCase.
              "camelCase": true,

              // Allow or disallow kebabCase.
              "kebabCase": true,

              // Allow or disallow pascalCase.
              "pascalCase": true,

              // Allow or disallow snakeCase.
              "snakeCase": true
            }

            // Filenames to ignore.
            // "ignore": []
          }
        ],

        // ESLint rule enforcing function references are not passed directly to iterator methods.
        "unicorn/no-fn-reference-in-iterator": "off",

        // ESLint rule enforcing 'null' is not used.
        "unicorn/no-null": "off",

        // ESLint rule enforcing 'undefined' is not used when unnecessary.
        "unicorn/no-useless-undefined": "off",

        // ESLint rule enforcing the disuse of abbreviations.
        "unicorn/prevent-abbreviations": "off"
      }

      // ESLint supports adding shared settings into configuration file.
      // You can add settings object to ESLint configuration file and it will be supplied to every rule that will be executed.
      // This may be useful if you are adding custom rules and want them to have access to the same information and be easily configurable.
      // "settings": {}
    },


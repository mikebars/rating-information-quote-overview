  // you can use the extends property and specify the path to the file.
  // The path can be either relative or absolute.
  // "extends": [],

  // Set each global variable name equal
  // to true to allow the variable to be overwritten
  // or false to disallow overwriting.
  // "globals": {},

  // Tell ESLint to ignore specific files and directories.
  // Each value uses the same pattern as the `.eslintignore` file.
  "ignorePatterns": [
    "!**//.*.cjs",
    "!**//.*.js",
    "!**//.*.jsx",
    "!**//.*.mjs",
    "!**//.*.ts",
    "!**//.*.tsx",
    "!**/*.cjs",
    "!**/*.js",
    "!**/*.jsx",
    "!**/*.mjs",
    "!**/*.ts",
    "!**/*.tsx",
    "**/.next/**",
    "**/coverage/**",
    "**/node_modules/**",
    "**/out/**",
    "**/patches/**",
    "**/public/**"
  ],


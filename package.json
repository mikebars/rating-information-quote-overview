  "scripts": {
    "/** `react-scripts` */": "",
    "start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "CI=true SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jest-environment-jsdom-sixteen",
    "/** `deploy` */": "",
    "deploy": "yarn lint && yarn test && yarn build && gh-pages --dist build",
    "/** `serve` */": "",
    "serve": "PUBLIC_URL=/ yarn build && serve --single --listen 3000 build",
    "/** `jest` */": "",
    "jest": "jest",
    "jest:ci": "CI=true yarn jest --ci",
    "jest:coverage": "yarn jest:ci --coverage",
    "jest:updateSnapshot": "yarn jest:ci --updateSnapshot",
    "jest:watch": "yarn jest --watch",
    "/** `jest:exhaustive` */": "",
    "jest:exhaustive": "JEST_EXHAUSTIVE=true yarn jest",
    "jest:exhaustive:ci": "JEST_EXHAUSTIVE=true yarn jest:ci",
    "jest:exhaustive:coverage": "JEST_EXHAUSTIVE=true yarn jest:coverage",
    "jest:exhaustive:updateSnapshot": "JEST_EXHAUSTIVE=true yarn jest:updateSnapshot",
    "jest:exhaustive:watch": "JEST_EXHAUSTIVE=true yarn jest:watch",
    "/** `lint` */": "",
    "lint": "yarn prettier && yarn eslint && yarn stylelint && yarn tslint && yarn tsc",
    "eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "yarn eslint --fix",
    "prettier": "prettier --loglevel error --write .",
    "stylelint": "yarn stylelint:css && yarn stylelint:css-in-js",
    "stylelint:css": "STYLELINT_NO_CSS_IN_JS=true stylelint \"**/*.css\" --allow-empty-input",
    "stylelint:css-in-js": "STYLELINT_NO_CSS_IN_JS=false stylelint \"**/*.{js,jsx,ts,tsx}\" --allow-empty-input",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "yarn tslint --fix",
    "/** `clean` */": "",
    "clean": "yarn clean-build && yarn clean-coverage && yarn clean-node && yarn clean-npm && yarn clean-yarn",
    "clean-build": "rm --force --recursive build/",
    "clean-coverage": "rm --force --recursive coverage/",
    "clean-node": "rm --force --recursive node_modules/",
    "clean-npm": "rm --force npm-debug.log && rm --force npm-shrinkwrap.json && rm --force package-lock.json && npm cache clear --force",
    "clean-yarn": "rm --force yarn.lock && rm --force yarn-debug.log && rm --force yarn-error.log && yarn cache clean"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

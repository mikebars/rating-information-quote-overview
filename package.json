  "scripts": {
    "|---------------------------------------------------------------------------|": "",
    "|--Application--------------------------------------------------------------|": "",
    "|--`react-app`--------------------------------------------------------------|": "",
    "react-app:development": "yarn react-scripts:start",
    "react-app:production": "yarn react-scripts:build && yarn react-scripts:serve",
    "react-app:static": "PUBLIC_URL=/ yarn react-scripts:build && yarn react-scripts:serve",
    "|--`react-scripts`----------------------------------------------------------|": "",
    "react-scripts:build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "react-scripts:serve": "serve build --listen 3000 --no-clipboard --single",
    "react-scripts:start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "react-scripts:test": "CI=true SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jest-environment-jsdom-sixteen",
    "|--DevOps-------------------------------------------------------------------|": "",
    "|--`deploy`-----------------------------------------------------------------|": "",
    "deploy": "yarn lint && yarn test && yarn react-scripts:build && yarn deploy:gh-pages",
    "deploy:gh-pages": "gh-pages --dist build",
    "|--Life Cycle---------------------------------------------------------------|": "",
    "postadd": "yarn patch",
    "postinstall": "yarn patch",
    "postremove": "yarn patch",
    "|--Git----------------------------------------------------------------------|": "",
    "|--`git-hooks`--------------------------------------------------------------|": "",
    "git-hooks:applypatch-msg": "",
    "git-hooks:commit-msg": "yarn commitlint --env HUSKY_GIT_PARAMS",
    "git-hooks:fsmonitor-watchman": "",
    "git-hooks:p4-pre-submit": "",
    "git-hooks:post-applypatch": "",
    "git-hooks:post-checkout": "",
    "git-hooks:post-commit": "",
    "git-hooks:post-merge": "",
    "git-hooks:post-rewrite": "",
    "git-hooks:post-update": "",
    "git-hooks:pre-applypatch": "",
    "git-hooks:pre-auto-gc": "",
    "git-hooks:pre-commit": "yarn patch && yarn lint-staged",
    "git-hooks:pre-push": "",
    "git-hooks:pre-rebase": "",
    "git-hooks:prepare-commit-msg": "exec < /dev/tty && yarn lint:commitizen --hook || true",
    "git-hooks:push-to-checkout": "",
    "git-hooks:sendemail-validate": "",
    "|--Tools--------------------------------------------------------------------|": "",
    "|--`lint`-------------------------------------------------------------------|": "",
    "lint": "yarn patch && yarn lint:prettier . && yarn lint:eslint . && yarn lint:stylelint:css . && yarn lint:stylelint:css-in-js . && yarn lint:tslint && yarn lint:tsc",
    "lint:commitizen": "cz",
    "lint:commitlint": "commitlint --config .commitlintrc.json",
    "lint:eslint": "NODE_ENV=development eslint --cache --cache-location .eslintcache --config .eslintrc.json --max-warnings 0 --no-error-on-unmatched-pattern",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:lint-staged": "lint-staged --concurrent false --config .lintstagedrc.json",
    "lint:prettier": "prettier --list-different --loglevel error",
    "lint:prettier:write": "yarn lint:prettier --write",
    "lint:stylelint": "stylelint --allow-empty-input --cache --cache-location .stylelintcache --ignore-pattern \"**/*.*\" --max-warnings 0",
    "lint:stylelint:css": "STYLELINT_NO_CSS_IN_JS=true yarn lint:stylelint --ignore-pattern \"!**/*.css\" --ignore-pattern \"!**/*.sass\" --ignore-pattern \"!**/*.scss\"",
    "lint:stylelint:css-in-js": "STYLELINT_NO_CSS_IN_JS=false yarn lint:stylelint --ignore-pattern \"!**/*.cjs\" --ignore-pattern \"!**/*.js\" --ignore-pattern \"!**/*.jsx\" --ignore-pattern \"!**/*.mjs\" --ignore-pattern \"!**/*.ts\" --ignore-pattern \"!**/*.tsx\"",
    "lint:tsc": "tsc --generateCpuProfile profile.cpuprofile --project tsconfig.json",
    "lint:tslint": "tslint --project tsconfig.json",
    "lint:tslint:fix": "yarn lint:tslint --fix",
    "|--`test`-------------------------------------------------------------------|": "",
    "test": "yarn patch && yarn test:jest:coverage",
    "test:jest": "jest",
    "test:jest:exhaustive": "JEST_EXHAUSTIVE=true yarn test:jest",
    "test:jest:ci": "CI=true yarn test:jest --ci",
    "test:jest:ci:exhaustive": "JEST_EXHAUSTIVE=true yarn test:jest:ci",
    "test:jest:coverage": "yarn test:jest:ci --coverage",
    "test:jest:coverage:exhaustive": "JEST_EXHAUSTIVE=true yarn test:jest:coverage",
    "test:jest:updateSnapshot": "yarn test:jest:ci --updateSnapshot",
    "test:jest:updateSnapshot:exhaustive": "JEST_EXHAUSTIVE=true yarn test:jest:updateSnapshot",
    "test:jest:watch": "yarn test:jest --watch",
    "test:jest:watch:exhaustive": "JEST_EXHAUSTIVE=true yarn test:jest:watch",
    "test:jest:with:all": "JEST_WITH_ALL=true yarn test:jest:coverage",
    "test:jest:with:all:exhaustive": "JEST_EXHAUSTIVE=true yarn test:jest:with:all",
    "test:jest:with:eslint": "JEST_WITH_ESLINT=true yarn test:jest:coverage",
    "test:jest:with:eslint:exhaustive": "JEST_EXHAUSTIVE=true yarn test:jest:with:eslint",
    "|--Utilities----------------------------------------------------------------|": "",
    "|--`clean`------------------------------------------------------------------|": "",
    "clean": "yarn clean:eslint && yarn clean:jest && yarn clean:next && yarn clean:node && yarn clean:npm && yarn clean:stylelint && yarn clean:tsc && yarn clean:yarn",
    "clean:eslint": "rm --force .eslintcache",
    "clean:jest": "rm --force --recursive .jestcache/ coverage/",
    "clean:next": "rm --force --recursive .next/ out/",
    "clean:node": "rm --force --recursive **/node_modules/.** **/node_modules/**",
    "clean:npm": "rm --force npm-*.log npm-shrinkwrap.json package-lock.json && npm cache clear --force",
    "clean:stylelint": "rm --force .stylelintcache",
    "clean:tsc": "rm --force *.cpuprofile *.tsbuildinfo .cpuprofile .tsbuildinfo",
    "clean:yarn": "rm --force yarn-*.har yarn-*.log yarn.lock && yarn cache clean",
    "|--`node`-------------------------------------------------------------------|": "",
    "node:inspect": "NODE_OPTIONS=\"--inspect=127.0.0.1:9229\" $($1)",
    "node:max-old-space-size:1GB": "NODE_OPTIONS=\"--max-old-space-size=1024\" $($1)",
    "node:max-old-space-size:2GB": "NODE_OPTIONS=\"--max-old-space-size=2048\" $($1)",
    "node:max-old-space-size:3GB": "NODE_OPTIONS=\"--max-old-space-size=3072\" $($1)",
    "node:max-old-space-size:4GB": "NODE_OPTIONS=\"--max-old-space-size=4096\" $($1)",
    "node:max-old-space-size:8GB": "NODE_OPTIONS=\"--max-old-space-size=8192\" $($1)",
    "node:max-old-space-size:16GB": "NODE_OPTIONS=\"--max-old-space-size=16384\" $($1)",
    "node:max-old-space-size:32GB": "NODE_OPTIONS=\"--max-old-space-size=32768\" $($1)",
    "node:max-old-space-size:64GB": "NODE_OPTIONS=\"--max-old-space-size=65536\" $($1)",
    "node:max-old-space-size:128GB": "NODE_OPTIONS=\"--max-old-space-size=131072\" $($1)",
    "node:max-old-space-size:256GB": "NODE_OPTIONS=\"--max-old-space-size=262144\" $($1)",
    "node:max-old-space-size:512GB": "NODE_OPTIONS=\"--max-old-space-size=524288\" $($1)",
    "node:max-old-space-size:1TB": "NODE_OPTIONS=\"--max-old-space-size=1048576\" $($1)",
    "|--`patch`------------------------------------------------------------------|": "",
    "patch": "yarn silent yarn patch:patch-package",
    "patch:patch-package": "patch-package --exclude --patch-dir patches --use-yarn",
    "|--`reset`------------------------------------------------------------------|": "",
    "reset": "yarn clean && yarn install && yarn postinstall",
    "|--`silent`-----------------------------------------------------------------|": "",
    "silent": "$($1) > /dev/null 2>&1",
    "|___________________________________________________________________________|": ""
  },
